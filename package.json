    },
    {
      "name": "Sergey Onufrienko <sergey@onufrienko.com>",
      "url": "https://github.com/sonufrienko"
    }
  ],
  "main": "src/api/server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cezerin/cezerin"
  },
  "scripts": {
    "build-api": "rm -rf ./build; babel . --out-dir build --ignore node_modules",
    "build-web": "rm -rf ./build; webpack --optimize-minimize --colors --progress",
    "heroku-postbuild": "bash post_build.sh",
    "watch": "webpack-dev-server --hot --colors --port 3000 --config webpack.dev.config.babel.js",
    "serve-worker": "node ./build/src/workers/product-batch-worker.js",
    "serve-api": "node ./build/src/api/server/index.js"
  },
